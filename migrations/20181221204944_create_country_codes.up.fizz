create_table("country_codes") {
	t.Column("alpha_2_code", "char(2)", {primary: true})
	t.Column("alpha_3_code", "char(3)", {"null": true})
	t.Column("numeric_code", "int", {"null": true})
	t.Column("international_name", "varchar(255)", {"null": true})
	t.Column("is_independant", "tinyint", {"null": true})
	t.Column("iso_status", "varchar(25)", {"null": true})
}

